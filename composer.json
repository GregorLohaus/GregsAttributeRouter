{
  "name": "gregs/attribute-router",
  "description": "Attribute based routing for laravel.",
  "type": "library",
  "homepage": "https://github.com/GregorLohaus/GregsAttributeRouter",
  "keywords": [
    "laravel",
    "routing",
    "attributes"
  ],
  "require": {
    "laravel/framework": "^11.0",
    "composer/composer": "^2.8",
    "php": "^8.3"
  },
  "require-dev": {
    "phpunit/phpunit": "12.1.x-dev",
    "phpstan/phpstan": "2.1.x-dev",
    "friendsofphp/php-cs-fixer": "dev-master",
    "mockery/mockery": "2.0.x-dev"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Gregs\\AttributeRouter\\": "src/",
      "Gregs\\AttributeRouter\\Tests\\": "tests/"
    },
    "classmap": [
      "tests/BasicClasses.php"
    ]
  },
  "scripts": {
    "stan": [
      "vendor/bin/phpstan analyze -c phpstan.neon"
    ],
    "test": [
      "XDEBUG_MODE=coverage vendor/bin/phpunit --testdox --coverage-text --coverage-clover clover.xml tests"
    ],
    "fmt": [
      "vendor/bin/php-cs-fixer fix src"
    ]
  },
  "authors": [
    {
      "name": "Gregor Lohaus",
      "email": "lohausgregor@gmail.com"
    }
  ],
  "minimum-stability": "dev"
}